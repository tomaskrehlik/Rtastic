<span class="symbol">gasw</span> <span class="assignement">&lt;-</span>  <span class="functioncall"><a href='plm.html'>plm</a></span><span class="keyword">(</span><span class="symbol">form</span><span class="keyword">,</span> <span class="argument">data</span> <span class="argument">=</span> <span class="symbol">Gasoline</span><span class="keyword">,</span> <span class="argument">model</span> <span class="argument">=</span> <span class="string">"within"</span><span class="keyword">)</span>
<span class="symbol">gasp</span> <span class="assignement">&lt;-</span>  <span class="functioncall"><a href='plm.html'>plm</a></span><span class="keyword">(</span><span class="symbol">form</span><span class="keyword">,</span> <span class="argument">data</span> <span class="argument">=</span> <span class="symbol">Gasoline</span><span class="keyword">,</span> <span class="argument">model</span> <span class="argument">=</span> <span class="string">"pooling"</span><span class="keyword">)</span>
